/dts-v1/;
/plugin/;
/ {
    compatible = "$platform";
    fragment@0{
        target = <&pwmtacho>;
        __overlay__{
            #address-cells = <1>;
            #size-cells = <0>;
            status = "okay";
            oemname$index = "$name";
            fan@$index {
                reg = <$reg>;
                aspeed,fan-tach-ch = /bits/ 8 <$tachs>;
            };
        };

    };
};